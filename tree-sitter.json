{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "leo",
      "camelcase": "Leo",
      "title": "Leo",
      "scope": "source.leo",
      "path": ".",
      "file-types": [
        "leo"
      ],
      "injection-regex": "leo",
      "class-name": "TreeSitterLeo",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Programming language for the Aleo blockchain",
    "authors": [
      {
        "name": "Kai Mast",
        "email": "kai@provable.com"
      }
    ],
    "links": {
      "repository": "https://github.com/kaimast/leo-grammar"
    }
  },
  "theme": {
    "keyword": "purple",
    "identifier": "green",
    "comment": "blue",
    "type": "orange",
    "function": "yellow"
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
